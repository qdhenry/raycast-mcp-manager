{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mcp-manager",
  "title": "MCP Manager",
  "description": "Add Model Context Protocol (MCP) configurations to Claude Code",
  "icon": "extension-icon.png",
  "author": "qdhenry",
  "categories": ["Developer Tools", "Productivity"],
  "license": "MIT",
  "commands": [
    {
      "name": "add-mcp",
      "title": "Add MCP Command",
      "subtitle": "MCP Manager",
      "description": "Parse and add an MCP command to your configuration",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}